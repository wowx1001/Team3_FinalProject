  <script>
	$(function(){
		var arr = new Array();
		{% for i in range(15)%}
			arr[{{i}}] = document.getElementById("image"+String({{i+1}}));
			arr[{{i}}].src = "static/img/flower/"+'{{ result2[i] }}'+".png";
			$("#image"+String({{i+1}})).tooltip({html: true, 
			title: {% for j in result2%}
						{% if j['f_num']==result2[i]%}"꽃 이름: "+"{{result[result.index(j)]['f_name']}}"+"<br>탄생화 날짜: "+{% if result[result.index(j)]['f_birth']=='' %}"없음"{% else %}"{{result[result.index(j)]['f_birth']}}"{% endif %}+"<br>꽃말: "+"{{result[result.index(j)]['f_lang']}}"{% endif %}{% endfor %}});
		{% endfor %}
		"{{}}"
		
	});
	$(function(){
		$(".fback").css("background-image","url('static/img/back"+String(Math.floor(Math.random()*6)+1)+".png')");
	});
  </script>
